{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
  { "handle": "filesystem" },
  { "src": "/assets/(.*)", "headers": { "cache-control": "public, max-age=31536000, immutable" }, "dest": "/assets/$1" },
  { "src": "\/(.*).(js|css|png|jpg|jpeg|svg|webp|woff2)", "headers": { "cache-control": "public, max-age=31536000, immutable" }, "dest": "\/$1.$2" },
  { "src": "\/_next/static/(.*)", "headers": { "cache-control": "public, max-age=31536000, immutable" }, "dest": "\/_next/static/$1" },
  { "src": "\/(.*)", "dest": "/index.html" }
  ]
}
